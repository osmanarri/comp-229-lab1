
<%- include ../partials/header.ejs %>

<!-- Content for the Add page -->




<div class="edit">
    <form  method= "POST" class="box">
      <p>
        <label for="contactName">Contact Name</label>
        <input type="text" id="contactName" name="contactName" value="<%= book.name %>" placeholder=" Contact name" autofocus/><br />
      </p>
      <p>
        <label for="contactNumber">Contact Number</label>
        <input type="text" id="contactNumber" name="contactNumber" value="<%= book.number %>" placeholder=" Contact number" /><br />
      </p>
      <p>
        <label for="emailAddress">Email</label>
        <input type="email" id="emailAddress" name="emailAddress" value="<%= book.email %>" placeholder="example@email.com"/><br />
      </p>
      
       <button>Update</button> 

        <button class="delete">Cancell</button>        
      
    </form>
</div>


